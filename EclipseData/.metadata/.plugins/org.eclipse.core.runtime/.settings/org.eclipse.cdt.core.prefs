build.all.configs.enabled=false
build.proj.ref.configs.enabled=false
doctool/workspace.default=org.eclipse.cdt.ui.doxygen
eclipse.preferences.version=1
includes.beginExportsPattern=IWYU\\s+(pragma\:?\\s+)?begin_exports?
includes.endExportsPattern=IWYU\\s+(pragma\:?\\s+)?end_exports?
includes.exportPattern=IWYU\\s+(pragma\:?\\s+)?export
includes.keepPattern=IWYU\\s+(pragma\:?\\s+)?keep
includes.privatePattern=IWYU\\s+(pragma\:?\\s+)?private(,\\s+include\\s+(?<header>\\S+))?
indexer/indexAllFiles=true
indexer/indexAllHeaderVersions=false
indexer/indexAllVersionsSpecificHeaders=
indexer/indexOnOpen=false
indexer/indexUnusedHeadersWithDefaultLang=true
indexer/indexerId=org.eclipse.cdt.core.fastIndexer
indexer/skipFilesLargerThanMB=8
indexer/skipImplicitReferences=false
indexer/skipIncludedFilesLargerThanMB=16
indexer/skipMacroReferences=false
indexer/skipReferences=false
indexer/skipTypeReferences=false
indexer/updatePolicy=0
indexer/useHeuristicIncludeResolution=true
org.eclipse.cdt.core.showSourceFilesInBinaries=true
org.eclipse.cdt.core.showSourceRootsAtTopLevelOfProject=true
org.eclipse.cdt.core.taskPriorities=normal,normal,normal
org.eclipse.cdt.core.taskTags=TODO,FIXME,XXX
