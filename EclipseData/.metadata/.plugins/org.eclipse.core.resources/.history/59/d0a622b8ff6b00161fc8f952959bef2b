typedef struct music_icon_data_s
{
	__u32 res_id;
	HTHEME res_handle;
	void *res_buffer;
} music_icon_data_t;

typedef struct music_layer_ui_s
{
	__pixel_rgbfmt_t fb_fmt;
	SIZE fb_size;
	RECT src_rect;
	RECT scn_rect;
	__u32 reserved[32];
} music_layer_ui_t;

typedef struct music_list_ui_s
{
	RECT listbar_rect;
	RECT item_rect;

	RECT focus_icon_rect;
	RECT file_icon_rect;

	RECT text_rect;
	RECT list_line;
} music_list_ui_t;

typedef struct music_scrollbar_ui_s
{
	RECT BG_rect;
	RECT scroll_rect;
	__u32 head_height;
	__u32 body_height;
	__s32 tail_height;

} music_scrollbar_ui_t;



typedef struct music_headbar_ui_s
{
	RECT BG_rect;
	RECT acomp_rect;
	RECT play_mode_rect;
	RECT EQ_mode_rect;
	RECT BLight_rect;
	RECT volume_icon_rect;
	RECT volume_text_rect;
	RECT battery_rect;
	RECT sys_time_rect;
} music_headbar_ui_t;


typedef struct music_palyer_ui_s
